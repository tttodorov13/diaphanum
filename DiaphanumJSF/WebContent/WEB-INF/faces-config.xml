<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/public/home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>projectNew</from-outcome>
			<to-view-id>/pages/protected/projectNew.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>projects</from-outcome>
			<to-view-id>/pages/public/projects.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>documents</from-outcome>
			<to-view-id>/pages/public/documents.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>document</from-outcome>
			<to-view-id>/pages/public/document.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>feedback</from-outcome>
			<to-view-id>/pages/public/feedback.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>message</from-outcome>
			<to-view-id>/pages/public/message.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/pages/public/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/public/home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>menu</from-outcome>
			<to-view-id>/pages/public/menu.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/pages/protected/profile.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>profileView</from-outcome>
			<to-view-id>/pages/public/profileView.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>projectEdit</from-outcome>
			<to-view-id>/pages/protected/projectEdit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<application>
		<default-render-kit-id>PRIMEFACES_MOBILE</default-render-kit-id>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
</faces-config>
